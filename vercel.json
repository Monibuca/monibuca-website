{
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "Access-Control-Allow-Origin",
          "value": "*"
        },
        {
          "key": "Access-Control-Allow-Headers",
          "value": "content-type"
        },
        {
          "key": "Access-Control-Allow-Methods",
          "value": "DELETE,PUT,POST,GET,OPTIONS"
        }
      ]
    }
  ],
  "rewrites": [
    {
      "source": "/chat/(.*)",
      "destination": "/api/chat"
    },
    {
      "source": "/engine/(.*)",
      "destination": "/api/engine"
    },
    {
      "source": "/plugin/(.*)",
      "destination": "/api/plugin"
    },
    {
      "source": "/monibuca/(.*)",
      "destination": "/api/monibuca"
    },
    {
      "source": "/m7s/(.*)",
      "destination": "/api/monibuca"
    },
    {
      "source": "/docs/(.*)",
      "destination": "https://monibuca.com/docs"
    },
    {
      "source": "/(.*)",
      "destination": "/"
    }
  ]
}